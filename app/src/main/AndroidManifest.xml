<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fcp.reportebasuras">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logoappcap"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logoappcap"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".menuapp.MenuAppRBas"
            android:label="@string/title_activity_menu_app_r_bas"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".Registro" />
        <activity android:name=".Terminos" />
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>